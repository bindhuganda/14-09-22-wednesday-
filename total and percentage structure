#include <stdio.h> 
   
 struct student 
 { 
     char name [30]; 
     int  marks[ 5]; 
     int  total; 
     float percentage; 
 }; 
   
 int main() 
 { 
     struct student std; 
     int i; 
   
     printf("Enter name: "); 
     gets(std.name); 
   
     printf("Enter marks:\n"); 
     std.total=0; 
     for(i=0;i< 5;i++){ 
         printf("Marks in subject %d: ",i+1); 
         scanf("%d",&std.marks[i]); 
         std.total+=std.marks[i]; 
     } 
     std.percentage=(float)((float)std.total/(float)500)*100; 
   
     printf("\nName: %s \nTotal Marks: %d \nPercentage: %.2f",std.name,std.total,std.percentage); 
   
     return 0; 
 } 
  
  
         TEXT CASE: 
         Enter RollNo,: 12 
         Name :  rama  
         Three sub marks 30 40 50 
  
         Output:  
         rollno=12  
         Name=rama  
         m1=30 m2=40 m3=50 total=120 per=40.000000  
  
         Test Case 
  
         24, AA, 77,65,75 
         22, BB, 56,62,50 
         23,CC. 42,45,38 
         25, Annie 55,65,74 
         26, joy. 85,84,87
